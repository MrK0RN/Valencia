RewriteEngine On
RewriteBase /

# Redirect /catalog to catalog.php
RewriteRule ^catalog/?$ catalog.php [L,QSA]

# Redirect /property/{id} to property.php?id={id}
RewriteRule ^property/([0-9]+)/?$ property.php?id=$1 [L,QSA]

# Optional: Redirect trailing slashes
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^(.*)/$ /$1 [R=301,L]

